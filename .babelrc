{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 iOS major versions", "last 2 Chrome versions", "last 2 Firefox versions"]
      }
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-private-methods", 
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-proposal-optional-chaining",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-proposal-private-property-in-object",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}